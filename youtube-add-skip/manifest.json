{
    "manifest_version": 3,
    "version": "1.1",
    "name": "Avoid automated ads on YouTube",
    "description": "Automated ads skip in 3 seconds on YouTube",

    "content_scripts": [{
        "matches": ["https://www.youtube.com/*"],
        "js": ["jquery.min.js", "content.js"],
        "css": ["style.css"]
    }],
    "permissions": [
        "tabs",
        "activeTab"
    ]
}